# AWS Credentials (for rocket-lander-deploy user)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=ap-southeast-2
AWS_ACCOUNT_ID=

# Deploy User ARN (required for seed role creation)
DEPLOY_USER_ARN=arn:aws:iam::YOUR_ACCOUNT_ID:user/rocket-lander-deploy

# CloudFormation Role ARN (optional - defaults to rocket-lander-cfn-role)
# CFN_ROLE_ARN=arn:aws:iam::YOUR_ACCOUNT_ID:role/rocket-lander-cfn-role

# Custom Domain (prod only - optional)
# DOMAIN_NAME=rocketlander.com
# HOSTED_ZONE_ID=Z1234567890ABC
# CERTIFICATE_ARN=arn:aws:acm:us-east-1:xxx:certificate/xxx
